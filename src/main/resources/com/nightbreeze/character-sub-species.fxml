<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="360.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.nightbreeze.controller.characterCreation.CharacterSubSpeciesController">
    <children>
        <VBox alignment="CENTER" prefHeight="360.0" prefWidth="600.0">
            <children>
                <Label fx:id="speciesNameLabel" text="Species Name">
                    <VBox.margin>
